                  {% if product.metafields.custom.zivnotnost_h_ %}
                    <tr class="technic-row">
                      <td class="technic-title">Životnost (v hodinách)</td>
                      <td class="technic-value">{{ product.metafields.custom.zivnotnost_h_ }}</td>
                    </tr>
                  {% endif %}
                  {% if product.metafields.custom.zivotnost %}
                    <tr class="technic-row">
                      <td class="technic-title">Životnost</td>
                      <td class="technic-value">{{ product.metafields.custom.zivotnost }}</td>
                    </tr>
                  {% endif %}
                  {% if product.metafields.custom.elektricka_trida %}
                    <tr class="technic-row">
                      <td class="technic-title">Elektrická třída</td>
                      <td class="technic-value">{{ product.metafields.custom.elektricka_trida }}</td>
                    </tr>
                  {% endif %}
                  {% if product.metafields.custom.stmivatelnost %}
                    <tr class="technic-row">
                      <td class="technic-title">Stmívatelné</td>
                      <td class="technic-value">{{ product.metafields.custom.stmivatelne | replace: "true", "ano" | replace: "false", "ne" }}</td>
                    </tr>
                  {% endif %}
                  {% if product.metafields.custom.pohybove_cidlo %}
                    <tr class="technic-row">
                      <td class="technic-title">Pohybové čidlo</td>
                      <td class="technic-value">{{ product.metafields.custom.pohybove_cidlo | replace: "true", "ano" | replace: "false", "ne" }}</td>
                    </tr>
                  {% endif %}



                  {% for variant in product.variants %}
                  {% if variant.metafields.custom.rozmer %}
                    <tr class="technic-row rozmer-varianty" id="{{ variant.id }}1" style="display:none;">
                      <td class="technic-title">Rozměry</td>
                      <td class="technic-value variant-rozmer">{{ variant.metafields.custom.rozmer }}</td>
                    </tr>
                    {% endif %}
                  {% endfor %}


                  {% if product.metafields.custom.rozmery %}
                    <tr class="technic-row">
                      <td class="technic-title">Rozměry</td>
                      <td class="technic-value">{{ product.metafields.custom.rozmery }}</td>
                    </tr>
