<section class="product-details-page">
  <div class="container">
    <div class="row">
    <div class="product-breadcrumb">
        <ul class="breadcrumb-list">
          <li class="breadcrumb-item">
            <a href="/" title="{{ 'general.breadcrumbs.home_link_title' | t }}">{{ 'general.breadcrumbs.home' | t }}</a>
          </li>
          {% if template contains 'product' %}
          {% if collection %}
          {% if collection.handle %}
          {% capture url %}/collections/{{ collection.handle }}{% endcapture %}
          <li class="breadcrumb-item">
            {{ collection.title | link_to: url }}
          </li>
          {% endif %}
          {% endif %}
          <li class="breadcrumb-item">
            <span>{{ product.title | truncate: 30 }}</span>
          </li>
          {% endif %}
        </ul>
      </div>
      </div></div>
<div class="product-beige">
  <div class="container">
    <div class="row">
      <div class="title-wrap desktop-hide">
        <div class="product-title">
          <h6>{{ product.title }}</h6>
        </div>
      </div>
      <div class="product-infos desktop-hide product-sku-mobile">
        <div>
          <h6>{{ section.settings.product_vendor_title }}</h6>
          <span class="variant-sku">{{ product.vendor }}</span>
        </div>
        <div>
          <h6>{{ section.settings.product_sku_title }}</h6>
          <span class="variant-sku" id="variantsku2">{{ current_variant.sku }}</span>
        </div>
      </div>
      {% if section.settings.product_style_handle == 'style_1' %}
      <div class="col-xl-6 col-lg-6 col-md-12 col-12 product_details product_details_gallery">
        <div class="product_detail_img">
            {% for t in product.tags %}
            {% if t contains 'novinka' %}     
              <div class="product-detail_badge badge-text" style="color:#86BAF5">
                Novinka
            </div>
            {% endif %}
         {% endfor %}
         {% if on_sale %}
          <div class="product-detail_badge badge-text" style="color:#FD1342">
            Akce
          </div>
         {% endif %}
          <!-- product_big_images start -->
          <div class="product_img_top">
            <div class="slider-for" {% if settings.enable_rtl %}dir="rtl"{% endif %} id="slider-for">
              {%- assign featured_media = current_variant.featured_media | default: product.featured_media -%}
              {%- for media in product.media -%}
              {% include 'media2', media: media, featured_media: featured_media %}
              {%- endfor -%}
            </div>
          </div>
          <!-- product_big_images end -->
          <!-- Start Small images -->
          <div class="product_slider_img">
            <div class="product_more_img">
              <div class="slider-nav pro-detail-slider gallery-thumbs product-single__thumbnails-{{ section.id }}" id="pro-detail-slider" {% if settings.enable_rtl %}dir="rtl"{% endif %}>
                  {% for media in product.media %}
                  <div class="slick-slide" data-image-id="{{ media.id }}" data-image="{{ media.src | img_url: 'master' }}">
                    <a class="product-single__thumbnail" href="javascript:void(0)"
                       class="text-link product-single__thumbnail"             
                       {% if enable_image_zoom %}data-zoom="{{ media.preview_image | img_url: 'master' }}"{% endif %}>
                      <img class="product-single__thumbnail-image img-fluid" src="{{ media.preview_image | img_url: 'master' }}" alt="{{ thumbnailAlt }}">
                      {%- if media.media_type == 'video' or media.media_type =='external_video' -%}
                      <span class="icon-play">
                      <i class="ti-control-play"></i>
                      </span>
                      {%- endif -%}
                      {%- if media.media_type == 'model' -%}
                      <div class="product-single__thumbnail-badge">
                        <i class="ti-box"></i>
                      </div>
                      {%- endif -%}
                    </a>
                  </div>
                  {% endfor %}
              </div>
            </div>
          </div>
        </div>
      </div>
      {% endif %}
      {% if section.settings.product_style_handle == 'style_2' %}
      <div class="col-xl-6 col-lg-6 col-md-12 col-12 product_details product_details_gallery">
        <div class="product_detail_img">
          <!-- product_big_images start -->
          <div class="product_img_top">
            <div class="slider-for" {% if settings.enable_rtl %}dir="rtl"{% endif %}>
              {%- assign featured_media = current_variant.featured_media | default: product.featured_media -%}
              {%- for media in product.media -%}
              {% include 'media', media: media, featured_media: featured_media, height: height, enable_image_zoom: enable_image_zoom, image_zoom_size: product_image_zoom_size, image_scale: product_image_scale %}
              {%- endfor -%}
            </div>
          </div>
          <!-- product_big_images end -->
          <!-- Start Small images -->
          <div class="product_slider_img">
            <div class="product_more_img">
              <div class="slider-nav pro-detail-slider gallery-thumbs product-single__thumbnails-{{ section.id }}" id="pro-detail-slider" {% if settings.enable_rtl %}dir="rtl"{% endif %}>
                  {% for media in product.media %}
                  <div class="slick-slide" data-image-id="{{ media.id }}" data-image="{{ media.src | img_url: 'master' }}">
                    <a class="product-single__thumbnail" href="javascript:void(0)"
                       class="text-link product-single__thumbnail"             
                       {% if enable_image_zoom %}data-zoom="{{ media.preview_image | img_url: 'master' }}"{% endif %}>
                      <img class="product-single__thumbnail-image img-fluid" src="{{ media.preview_image | img_url: 'master' }}" alt="{{ thumbnailAlt }}">
                      {%- if media.media_type == 'video' or media.media_type =='external_video' -%}
                      <span class="icon-play">
                        <i class="ti-control-play"></i>
                      </span>
                      {%- endif -%}
                      {%- if media.media_type == 'model' -%}
                      <div class="product-single__thumbnail-badge">
                        <i class="ti-box"></i>
                      </div>
                      {%- endif -%}
                    </a>
                  </div>
                  {% endfor %}
              </div>
            </div>
          </div>
        </div>
      </div>
      {% endif %}
      {% if section.settings.product_style_handle == 'style_3' %}
      <div class="col-xl-6 col-lg-6 col-md-12 col-12 product_details product_details_gallery">
        <div class="product_detail_img">
          <!-- product_big_images start -->
          
          <!-- product_big_images end -->
          <!-- Start Small images -->
          <div class="product_slider_img">
            <div class="product_more_img">
              <div class="slider-nav pro-detail-slider gallery-thumbs product-single__thumbnails-{{ section.id }}" id="pro-detail-slider" {% if settings.enable_rtl %}dir="rtl"{% endif %}>
                  {% for media in product.media %}
                  <div class="slick-slide" data-image-id="{{ media.id }}" data-image="{{ media.src | img_url: 'master' }}">
                    <a class="product-single__thumbnail" href="javascript:void(0)"
