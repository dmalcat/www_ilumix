                  {% if product.metafields.custom.index_barev %}
                    <tr class="technic-row">
                      <td class="technic-title">Index podání barev (CRI)</td>
                      <td class="technic-value">{{ product.metafields.custom.index_barev }}</td>
                    </tr>
                  {% endif %}
                  {% if product.metafields.custom.uhel_svitu %}
                    <tr class="technic-row">
                      <td class="technic-title">Úhel svitu</td>
                      <td class="technic-value">{{ product.metafields.custom.uhel_svitu }}</td>
                    </tr>
