                       {% if enable_image_zoom %}data-zoom="{{ media.preview_image | img_url: 'master' }}"{% endif %}>
                      <img class="product-single__thumbnail-image img-fluid" src="{{ media.preview_image | img_url: 'master' }}" alt="{{ thumbnailAlt }}">
                      {%- if media.media_type == 'video' or media.media_type =='external_video' -%}
                      <span class="icon-play">
                      <i class="ti-control-play"></i>
                      </span>
                      {%- endif -%}
                      {%- if media.media_type == 'model' -%}
                      <div class="product-single__thumbnail-badge">
                        <i class="ti-box"></i>
                      </div>
                      {%- endif -%}
                    </a>
                  </div>
                  {% endfor %}
              </div>
            </div>
          </div>
          <div class="product_img_top">
            <div class="slider-for" {% if settings.enable_rtl %}dir="rtl"{% endif %}>
                {%- assign featured_media = current_variant.featured_media | default: product.featured_media -%}
              {%- for media in product.media -%}
              {% include 'media', media: media, featured_media: featured_media, height: height, enable_image_zoom: enable_image_zoom, image_zoom_size: product_image_zoom_size, image_scale: product_image_scale %}
              {%- endfor -%}
            </div>
          </div>
        </div>
      </div>
      {% endif %}

      <div class="col-xl-6 col-lg-6 col-md-12 col-12 product_details product_details_content">
        <div class="product_details_info">
            {%- for block in section.blocks -%}
            {%- case block.type -%}
            {%- when '@app' -%}
            {% render block %}

            {%- when 'text' -%}
            {{- block.settings.text -}}

            {%- when 'title_wrap' -%}
            <div class="title-wrap mobile-hide">
              <div class="product-title">
                <h6>{{ product.title }}</h6>
              </div>
            {% include 'social-share' %}
            </div>
            <!-- <div class="product-ratting">
              <span class="shopify-product-reviews-badge" data-id="{{ product.id }}"></span>
            </div> -->
            <div class="product-infos mobile-hide">
                <h6>{{ section.settings.product_vendor_title }}</h6>
                <span class="variant-sku">{{ product.vendor }}</span>
              </div>
            <div class="product-infos mobile-hide">
                <h6>{{ section.settings.product_sku_title }}</h6>
                <span class="variant-sku" id="variantsku">{{ current_variant.sku }}</span>
              </div>
              
              {% if product.metafields.custom.typ_svetelneho_zdroje or product.metafields.custom.vykon or product.metafields.custom.svetelny_tok or product.metafields.custom.barevna_teplota or product.metafields.custom.stupen_kryti %}
              <span class="product-meta">
                <span class="product-meta__group product-meta__group--lumins">
                  {% if product.metafields.custom.typ_svetelneho_zdroje %}
                    <span>{{ product.metafields.custom.typ_svetelneho_zdroje }}</span>
                  {% endif %}

                  {% if product.metafields.custom.vykon %}
                    <span>&nbsp;{{ product.metafields.custom.vykon }}</span>
                  {% endif %}

                  {% if product.metafields.custom.svetelny_tok %}
                    <span>&nbsp;{{ product.metafields.custom.svetelny_tok }}</span>
                  {% endif %}

                  {% if product.metafields.custom.barevna_teplota %}
                    <span>&nbsp;{{ product.metafields.custom.barevna_teplota }}</span>
                  {% endif %}
                </span>
                <span class="product-meta__group product-meta__group--coverage">
                  {% if product.metafields.custom.stupen_kryti %}
                    <span>{{ product.metafields.custom.stupen_kryti }}</span>
                  {% endif %}
                </span>
              </span>
              {% endif %}

              <script>
                $( document ).ready(function() {
                  
                  // get selected option on load
                  var selectedCct = $('input[type=radio][name=option-0]:checked').val();

                  $( "input[type=radio][name=option-0]" ).change(function() {
                      var selectedCct = $(this).val();
                  });
                  

                  console.log(selectedCct);
                  
                });
              </script>
              
              {% if product.metafields.custom.psaci_text_pod_vyrobcem %}
                
                <div class="product-badge"><p>{{product.metafields.custom.psaci_text_pod_vyrobcem}}</p></div>
              {% endif %}
              <div class="product-desc mobile-hide">
                {{ product.description }}
              </div>
            {%- when 'short_description' -%}
            {% if product.metafields.my_fields.short_description %}
            <div class="available-desc">
              <div class="product-description">
                {{ product.metafields.my_fields.short_description }}
              </div>
            </div>
            {% endif %}
            
          {%- when 'product_availability' -%}
          {% assign colorVariant = product.metafields.my_fields.color_swatche.value %}
          {% if colorVariant %}

            <div class="swatch-custom-container">
              {% include 'product-swatch-custom' %}
            </div>
          {% endif %}
          <div class="pro-prlb{% if on_sale %} pro-sale{% endif %}">
            <div class="price-box">
              <h6>{{ section.settings.product_cena }}</h6>
              <div class="price-flex">
                {%- if wcp_compare_at_price_max > wcp_price -%}
                <span class="old-price" id="ComparePrice">{{ wcp_v_compare_at_price | money }}</span>
                {%- endif -%}
                <p><span class="new-price" id="ProductPrice">{{ wcp_v_price | money }}</span></p>
                <p class="price-no-vat"><span id="ProductPriceVat">{{ wcp_v_price | divided_by: 1.21 | money }}</span> bez DPH</p>
              </div>
            </div>
            {% if on_sale %}
            {% if settings.sale_percent_enable %}

              <div class="product-label">
                <span id="ProductDiscount">{{ wcp_v_compare_at_price | minus: wcp_v_price | times: 100.0 | divided_by: wcp_v_compare_at_price | money_without_currency | replace: ',', '.' | times: 100 | remove: '.0' }}</span>
              </div>

            {% endif %}
            {% endif %}
          </div>
            <div class="product-inventory" id="variant-inventory">
              <h6>{{ block.settings.availability_title }}</h6>
              {% if product.variants.first.inventory_management == "shopify" and product.variants.first.inventory_quantity == 0 %}
              <span class="out-stock text-danger">
                <span>{{ block.settings.out_of_stock_title }}</i></span>
              </span>
              <span class="in-stock text-success">
                <span>{{ block.settings.in_stock_title }}</span>
                {{ block.settings.stock_vice }}
              </span>
              {% elsif product.variants.first.inventory_management == "shopify" and product.variants.first.inventory_quantity > 0 %}
              <span class="in-stock text-success">
                <span>{{ block.settings.in_stock_title }}</span>
              </span>
              <span class="out-stock text-danger">
                <span>{{ block.settings.out_of_stock_title }}</span>
              </span>
              {% else %}
              <span class="in-stock d-block text-success">
                <span>{{ block.settings.in_stock_title }}</span>
              </span>
              {% endif %}
            </div>
            
            {%- when 'product_stock_inventory' -%}
              
            {% if product.variants.first.inventory_management == "shopify" and product.variants.first.inventory_quantity == 0 %}
            {% elsif product.variants.first.inventory_management == "shopify" and product.variants.first.inventory_quantity > 0 %}
            <div class="stock-inventory">
            <div class="stock-more text-success">
              {{ block.settings.stock_vice }}
            </div>
            <div class="stock-kus text-success">
              <span class="available-stock">{{ product.variants.first.inventory_quantity }}</span>
              <span>{{ block.settings.stock_kusy }}</span>
            </div>
            </div>
            {% endif %}
          

          {%- when 'product_variant_picker' -%}

          <div class="pro-detail-action pp-action">
            <form action="/cart/add" method="post" class="cart-{{ product.id }}" enctype="multipart/form-data" id="add-item-form">
              <div class="product-actions">
                <div class="product-quantity-action">
                  {% include 'product-quantity' %}
                  {% include 'product-cart-action' %}
                  <!-- {% include 'instant-buy' %} -->
                </div>
              </div>
            </form>
          </div>
            <div class="pro-detail-action pp-swatch">
              <form action="/cart/add" method="post" class="cart-{{ product.id }}" enctype="multipart/form-data" id="add-item-form">
                {% include 'product-swatch-variant' %}
              </form>
            </div>
          {%- when 'product_description_tab' -%}
          <div class="product-b2b">{{section.settings.velkoobchod}}</div>
          <div class="product-description-tab">
            <ul id="accordion">

              <li role="presentation">
                <a class="acc-title collapsed" href="#additional-information" data-bs-toggle="collapse">
                  {{ block.settings.additional_information_title }}
                </a>
                <div class="tab-content collapse" id="additional-information" data-bs-parent="#accordion">
              <table class="technic-table">
                <tbody>
                  {% if product.metafields.custom.varianty_montaze %}
                    <tr class="technic-row">
                      <td class="technic-title">Varianta montáže</td>
                      <td class="technic-value">{{ product.metafields.custom.varianty_montaze }}</td>
                    </tr>
                  {% endif %}
                  {% if product.metafields.custom.stupen_kryti %}
                    <tr class="technic-row">
                      <td class="technic-title">Stupeň krytí</td>
                      <td class="technic-value">{{ product.metafields.custom.stupen_kryti }}</td>
                    </tr>
                  {% endif %}
                  {% if product.metafields.custom.povrchova_uprava %}
                    <tr class="technic-row">
                      <td class="technic-title">Povrchová úprava</td>
                      <td class="technic-value">{{ product.metafields.custom.povrchova_uprava }}</td>
                    </tr>
                  {% endif %}
                  {% if product.metafields.custom.material %}
                    <tr class="technic-row">
                      <td class="technic-title">Materiál</td>
                      <td class="technic-value">{{ product.metafields.custom.material }}</td>
                    </tr>
                  {% endif %}
                  {% if product.metafields.custom.typ_svetelneho_zdroje %}
                    <tr class="technic-row">
                      <td class="technic-title">Typ světelného zdroje</td>
                      <td class="technic-value">{{ product.metafields.custom.typ_svetelneho_zdroje }}</td>
                    </tr>
                  {% endif %}
                  {% for variant in product.variants %}
                    {% if variant.metafields.custom.vykon %}
                      <tr class="technic-row vykon-varianty" id="{{ variant.id }}4" style="display:none;">
                        <td class="technic-title">Výkon</td>
                        <td class="technic-value variant-vykon">{{ variant.metafields.custom.vykon }}</td>
                      </tr>
                      {% endif %}
                    {% endfor %}

                  {% if product.metafields.custom.vykon %}
                    <tr class="technic-row">
                      <td class="technic-title">Výkon</td>
                      <td class="technic-value">{{ product.metafields.custom.vykon }}</td>
                    </tr>
                  {% endif %}

                  {% for variant in product.variants %}
                    {% if variant.metafields.custom.svetelny_tok %}
                      <tr class="technic-row tok-varianty" id="{{ variant.id }}5" style="display:none;">
                        <td class="technic-title">Světelný tok</td>
                        <td class="technic-value variant-tok">{{ variant.metafields.custom.svetelny_tok }}</td>
                      </tr>
                      {% endif %}
                    {% endfor %}

                  {% if product.metafields.custom.svetelny_tok %}
                    <tr class="technic-row">
                      <td class="technic-title">Světelný tok</td>
                      <td class="technic-value">{{ product.metafields.custom.svetelny_tok }}</td>
                    </tr>
