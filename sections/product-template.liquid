                        <td class="technic-value variant-barva">{{ variant.metafields.custom.barevna_teplota }}</td>
                      </tr>
                      {% endif %}
                    {% endfor %}
                  {% if product.metafields.custom.barevna_teplota %}
                    <tr class="technic-row">
                      <td class="technic-title">Barevná teplota</td>
                      <td class="technic-value">{{ product.metafields.custom.barevna_teplota }}</td>
                    </tr>
